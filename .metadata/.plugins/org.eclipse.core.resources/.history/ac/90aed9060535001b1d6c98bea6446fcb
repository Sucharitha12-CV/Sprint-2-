package com.cg.ohds.entity;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
/**
 * this class defines the details of the customer 
 * @author Sucharitha
 * @param customerId-defines id of customer
 * @param customerName-defines name of customer
 * @param customerUserName-defines userName of customer
 * @param customerPassword-defines password of customer
 * @param customerEmail-defines email of customer
 * @param customerPhoneNo-defines phoneNo of customer
 * @param customerId-defines customerId of customer
 */

@Entity
public class Customer {
	@Id
	@GeneratedValue(strategy=GenerationType.IDENTITY)
	private int customerId;
	private String customerName;
	private String customerUserName;
	private String customerPassword;
	private String customerEmail;
	private long customerPhoneNo;
	private Address customerAddress;

}
